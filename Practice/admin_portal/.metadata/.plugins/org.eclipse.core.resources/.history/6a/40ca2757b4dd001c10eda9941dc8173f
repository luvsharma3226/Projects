package com.ng.admin.model;

import java.util.ArrayList;
import java.util.List;

import javax.persistence.Entity;
import javax.persistence.JoinColumn;
import javax.persistence.JoinTable;
import javax.persistence.ManyToMany;

import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

@Getter
@Setter
@NoArgsConstructor
@AllArgsConstructor
@Entity
public class Mart extends BaseEntity{
	private String martName;
	@ManyToMany
	@JoinTable(joinColumns = @JoinColumn(name="mart_id"),inverseJoinColumns = @JoinColumn(name="mapping_id"))
	private List<MenuMapping> menuMappingList = new ArrayList<MenuMapping>();
}
