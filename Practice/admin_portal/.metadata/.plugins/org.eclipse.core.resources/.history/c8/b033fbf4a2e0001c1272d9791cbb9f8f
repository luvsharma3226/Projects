package com.ng.admin.model;

import java.util.ArrayList;
import java.util.List;

import javax.persistence.Entity;
import javax.persistence.JoinColumn;
import javax.persistence.JoinTable;
import javax.persistence.ManyToMany;

import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

@NoArgsConstructor
@AllArgsConstructor
@Getter
@Setter
@Entity
public class Portal extends BaseEntity{
	private String portalName;
	private String portalAlias;
	@ManyToMany
	@JoinTable( name="portal_menu_mapping", joinColumns = @JoinColumn(name="portal_id") ,inverseJoinColumns =  @JoinColumn(name="menu_mapping_id"))
	private List<MenuMapping> menuMappingList = new ArrayList<>();
}
